{
  "name": "dex-order-serialization-lib",
  "version": "0.0.1",
  "description": "A library to (de)serialize cbor from different DEXes (decentralized exchanges) to specific types for order transaction construction.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [
    "cbor",
    "plutus",
    "plutus core",
    "blockchain",
    "cardano",
    "serialization",
    "dex",
    "lace"
  ],
  "author": "William Wolff",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@emurgo/cardano-serialization-lib-nodejs": "^11.3.0"
  }
}
