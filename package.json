{
  "name": "@will991/dex-order-serialization-lib",
  "version": "1.0.0",
  "description": "A library to (de)serialize cbor from different DEXes (decentralized exchanges) to specific types for order transaction construction.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/will991/dex-order-serialization-lib.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "test:cov": "jest --config jestconfig.json --coverage",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cbor",
    "plutus",
    "plutus core",
    "blockchain",
    "cardano",
    "serialization",
    "dex",
    "lace"
  ],
  "author": "William Wolff",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "prettier": "^2.8.4",
    "semantic-release": "^21.0.1",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@emurgo/cardano-serialization-lib-nodejs": "^11.3.0"
  }
}
